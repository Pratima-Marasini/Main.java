package Problem3;

public class GeneralInfo {
	private int id, age;
	private String name, nationality;

	public GeneralInfo(int id,String name, int age,  String nationality) {
		super();
		this.id = id;
		this.age = age;
		this.name = name;
		this.nationality = nationality;
	}

	@Override
	public String toString() {
		return "GeneralInfo [id=" + id + ", age=" + age + ", name=" + name + ", nationality=" + nationality + "]";
	}

	
}
